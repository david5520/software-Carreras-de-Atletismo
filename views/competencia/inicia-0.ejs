
<div id="iniciarcompe" class="modal fade bd-example-modal-lg" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <form class="text-center" >
          <div class="modal-header">            
            <h4 class="modal-title">Elije una competencia para Empezar</h4>
            <button type="button" class="close" data-dismiss="modal" id="closeC" aria-hidden="true">&times;</button>
          </div>
          <div class="modal-body"> 
            
          <table class="table table-striped table-bordered table-fixed display"  id="Tablecompe" >
              <thead>
                <tr>
                  <th scope="col">ID</th><th scope="col">Nombre</th><th scope="col">Lugar</th><th scope="col">Fecha</th><th scope="col">HORA</th>
                </tr>
              </thead>
              <tbody>

                <% for(var i=0; i<datosCompetencia.length; i++) {%>
                <tr>
                <th scope="row" class="idCompetencia"><%= datosCompetencia[i]['id'] %></th>
                <td scope="row" class="nombreCompetencia"><%= datosCompetencia[i]['nombre'] %></td>
                <td class="lugarCompetencia"><%= datosCompetencia[i]['lugar'] %></td>
                <td class="fechaCompetencia"><%= datosCompetencia[i]['fecha'] %></td>
                <td class="horaCompetencia"><%= datosCompetencia[i]['hora'] %></td>
              </tr>
              <% } %>
             
              </tbody>
            </table>

          </div>
          <div class="modal-footer">
            <input type="button" class="btn btn-secondary  text-uppercase " data-dismiss="modal" id="cancelarC" value="Cancelar">
            <input type="button" class="btn btn-success" data-dismiss="modal" value="Empezar" id="btn-ini" onclick="mostrar();">
          </div>
        </form>
      </div>
    </div>
  </div>